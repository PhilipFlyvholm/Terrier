{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "rootDir": "src",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  }
}